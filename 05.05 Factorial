// Functions and Methods
//
// #5  Factorial:
//
// 
// 
// 

public class Factorial {
   
    public static int f(int n){
        if ( n > 1 )
            n = n * f (n-1);
        return n;
    }    
        
    public static void main(String []args){

        System.out.println(f(9));
        
     }
}
